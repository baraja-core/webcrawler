{
	"name": "baraja-core/webcrawler",
	"description": "Simple package to load list of urls and make sitemap.",
	"homepage": "https://github.com/baraja-core/webcrawler",
	"authors": [
		{
			"name": "Jan Barášek",
			"homepage": "https://baraja.cz"
		}
	],
	"require": {
		"php": "^8.0",
		"ext-curl": "*",
		"nette/utils": "^3.0",
		"nette/http": "^3.0"
	},
	"require-dev": {
		"phpstan/phpstan": "^0.12.74",
		"tracy/tracy": "^2.8",
		"phpstan/phpstan-nette": "^0.12.14",
		"roave/security-advisories": "dev-master"
	},
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"scripts": {
		"phpstan": [
			"vendor/bin/phpstan analyse src -c phpstan.neon --level 8 --no-progress"
		]
	},
	"minimum-stability": "stable"
}
